                                                                               "steps":  [
                                                                                             {
                                                                                                 "color":  "green",
                                                                                                 "value":  null
                                                                                             },
                                                                                             {
                                                                                                 "color":  "orange",
                                                                                                 "value":  0
                                                                                             },
                                                                                             {
                                                                                                 "color":  "red",
                                                                                                 "value":  10
                                                                                             }
                                                                                         ]
                                                                           }
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  12,
                                       "x":  0,
                                       "y":  32
