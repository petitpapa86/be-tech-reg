                   {
                       "id":  12,
                       "title":  "Resource Utilization",
                       "type":  "bargauge",
                       "targets":  [
                                       {
                                           "expr":  "avg(jvm_memory_used_bytes / jvm_memory_max_bytes) by (area)",
                                           "legendFormat":  "{{area}}",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "percent",
                                                            "min":  0,
                                                            "max":  1
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  12,
                                       "x":  12,
                                       "y":  32
