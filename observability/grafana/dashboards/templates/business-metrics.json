{
    "id":  null,
    "title":  "[Business] RegTech Platform Metrics",
    "tags":  [
                 "business",
                 "kpi",
                 "regtech",
                 "compliance"
             ],
    "timezone":  "browser",
    "panels":  [
                   {
                       "id":  1,
                       "title":  "Total API Requests (Today)",
                       "type":  "stat",
                       "targets":  [
                                       {
                                           "expr":  "sum(increase(http_server_requests_seconds_count[24h]))",
                                           "legendFormat":  "Total Requests",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "short"
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  6,
                                       "x":  0,
                                       "y":  0
                                   }
                   },
                   {
                       "id":  2,
                       "title":  "Active Users (Last Hour)",
                       "type":  "stat",
                       "targets":  [
                                       {
                                           "expr":  "count(count by (user_id) (http_server_requests_seconds_count{user_id!=\"\"}[1h]))",
                                           "legendFormat":  "Active Users",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "short"
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  6,
                                       "x":  6,
                                       "y":  0
                                   }
                   },
                   {
                       "id":  3,
                       "title":  "Risk Assessments Processed",
                       "type":  "stat",
                       "targets":  [
                                       {
                                           "expr":  "sum(increase(regtech_risk_assessments_total[1h]))",
                                           "legendFormat":  "Risk Assessments",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "short"
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  6,
                                       "x":  12,
                                       "y":  0
                                   }
                   },
                   {
                       "id":  4,
                       "title":  "Reports Generated",
                       "type":  "stat",
                       "targets":  [
                                       {
                                           "expr":  "sum(increase(regtech_reports_generated_total[1h]))",
                                           "legendFormat":  "Reports Generated",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "short"
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  6,
                                       "x":  18,
                                       "y":  0
                                   }
                   },
                   {
                       "id":  5,
                       "title":  "API Request Rate by Module",
                       "type":  "bargauge",
                       "targets":  [
                                       {
                                           "expr":  "sum(rate(http_server_requests_seconds_count[5m])) by (module)",
                                           "legendFormat":  "{{module}}",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "reqps"
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  12,
                                       "x":  0,
                                       "y":  8
                                   }
                   },
                   {
                       "id":  6,
                       "title":  "Business Transaction Success Rate",
                       "type":  "bargauge",
                       "targets":  [
                                       {
                                           "expr":  "(1 - sum(rate(regtech_business_transactions_failed_total[5m])) / sum(rate(regtech_business_transactions_total[5m]))) * 100",
                                           "legendFormat":  "Success Rate %",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "percent",
                                                            "thresholds":  {
                                                                               "mode":  "absolute",
                                                                               "steps":  [
                                                                                             {
                                                                                                 "color":  "red",
                                                                                                 "value":  null
                                                                                             },
                                                                                             {
                                                                                                 "color":  "orange",
                                                                                                 "value":  95
                                                                                             },
                                                                                             {
                                                                                                 "color":  "green",
                                                                                                 "value":  99
                                                                                             }
                                                                                         ]
                                                                           }
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  12,
                                       "x":  12,
                                       "y":  8
                                   }
                   },
                   {
                       "id":  7,
                       "title":  "Data Quality Score Trend",
                       "type":  "graph",
                       "targets":  [
                                       {
                                           "expr":  "avg(regtech_data_quality_score)",
                                           "legendFormat":  "Data Quality Score",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "percent",
                                                            "min":  0,
                                                            "max":  100
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  12,
                                       "x":  0,
                                       "y":  16
                                   }
                   },
                   {
                       "id":  8,
                       "title":  "Compliance Check Results",
                       "type":  "piechart",
                       "targets":  [
                                       {
                                           "expr":  "sum(regtech_compliance_checks_total) by (result)",
                                           "legendFormat":  "{{result}}",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "color":  {
                                                                          "mode":  "thresholds"
                                                                      },
                                                            "thresholds":  {
                                                                               "mode":  "absolute",
                                                                               "steps":  [
                                                                                             {
                                                                                                 "color":  "green",
                                                                                                 "value":  null
                                                                                             },
                                                                                             {
                                                                                                 "color":  "red",
                                                                                                 "value":  0
                                                                                             }
                                                                                         ]
                                                                           }
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  12,
                                       "x":  12,
                                       "y":  16
                                   }
                   },
                   {
                       "id":  9,
                       "title":  "Top Risk Categories",
                       "type":  "table",
                       "targets":  [
                                       {
                                           "expr":  "topk(10, sum(rate(regtech_risk_detected_total[1h])) by (category))",
                                           "legendFormat":  "{{category}}",
                                           "refId":  "A",
                                           "instant":  true
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "unit":  "short"
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  12,
                                       "x":  0,
                                       "y":  24
                                   }
                   },
                   {
                       "id":  10,
                       "title":  "Business Process Duration",
                       "type":  "heatmap",
                       "targets":  [
                                       {
                                           "expr":  "sum(rate(regtech_business_process_duration_seconds_bucket[5m])) by (le, process_type)",
                                           "legendFormat":  "{{process_type}}",
                                           "refId":  "A"
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "custom":  {
                                                                           "scaleDistribution":  {
                                                                                                     "type":  "log"
                                                                                                 }
                                                                       },
                                                            "unit":  "s"
                                                        }
                                       },
                       "gridPos":  {
                                       "h":  8,
                                       "w":  12,
                                       "x":  12,
                                       "y":  24
                                   }
                   },
                   {
                       "id":  11,
                       "title":  "Alert Summary",
                       "type":  "table",
                       "targets":  [
                                       {
                                           "expr":  "sum(increase(regtech_alerts_total[1h])) by (severity, type)",
                                           "legendFormat":  "{{severity}} - {{type}}",
                                           "refId":  "A",
                                           "instant":  true
                                       }
                                   ],
                       "fieldConfig":  {
                                           "defaults":  {
                                                            "thresholds":  {
