{
  "dashboard": {
    "id": null,
    "title": "[Business] API Performance by Module",
    "tags": ["business", "api", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate by Module and Endpoint",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count[5m])) by (module, uri)",
            "legendFormat": "{{module}} - {{uri}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "P95 Response Time by Module",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le, module))",
            "legendFormat": "{{module}} P95",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "API Error Rate by Module",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count{status=~\"4..|5..\"}[5m])) by (module) / sum(rate(http_server_requests_seconds_count[5m])) by (module) * 100",
            "legendFormat": "{{module}} error %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Ingestion Module API Performance",
