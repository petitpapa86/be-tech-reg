          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{module=\"ingestion\"}[5m])) by (le, uri))",
            "legendFormat": "{{uri}} P95",
            "refId": "B",
            "instant": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "auto"
            }
